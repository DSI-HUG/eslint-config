{
    "name": "@hug/eslint-config",
    "version": "22.0.0",
    "description": "ESLint shareable configuration with great defaults",
    "homepage": "https://github.com/dsi-hug/eslint-config",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsi-hug/eslint-config.git"
    },
    "publishConfig": {
        "access": "public",
        "tag": "latest"
    },
    "type": "module",
    "main": "./index.mjs",
    "typings": "./index.d.ts",
    "exports": {
        ".": {
            "require": "./index.mjs",
            "types": "./index.d.ts",
            "default": "./index.mjs"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "README.md",
        "LICENSE",
        "index.mjs",
        "index.d.ts",
        "configs",
        "scripts/deprecated.mjs"
    ],
    "keywords": [
        "eslint",
        "eslintconfig",
        "lint",
        "linter",
        "code",
        "style",
        "quality",
        "angular",
        "rxjs",
        "javascript",
        "jslint",
        "es6",
        "typescript",
        "cypress"
    ],
    "scripts": {
        "postinstall": "node scripts/deprecated.mjs",
        "ncu": "npx npm-check-updates -i --format=group",
        "inspect": "npx @eslint/config-inspector",
        "prepare": "husky || true",
        "lint": "eslint .",
        "test": "cross-env CONFIG=MODERATE eslint tests && cross-env CONFIG=RECOMMENDED eslint tests",
        "test:ci": "npm run test",
        "release": "npm publish"
    },
    "peerDependencies": {
        "@angular/core": ">= 10",
        "eslint": ">= 9",
        "rxjs": ">= 6",
        "typescript": ">= 3"
    },
    "dependencies": {
        "@stylistic/eslint-plugin": "5.6.1",
        "angular-eslint": "20.6.0",
        "chalk": "^5.6.2",
        "eslint-config-prettier": "10.1.8",
        "eslint-plugin-chai-friendly": "1.1.0",
        "eslint-plugin-cypress": "5.2.0",
        "eslint-plugin-jsdoc": "61.4.0",
        "eslint-plugin-jsonc": "2.21.0",
        "eslint-plugin-no-loops": "0.4.0",
        "eslint-plugin-no-secrets": "2.2.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-rxjs-angular-updated": "1.0.50",
        "eslint-plugin-rxjs-updated": "1.0.50",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-unused-imports": "4.3.0",
        "find-up": "^8.0.0",
        "globals": "^16.5.0",
        "typescript-eslint": "8.47.0"
    },
    "devDependencies": {
        "@angular/core": "^20.3.12",
        "@commitlint/cli": "^20.1.0",
        "@commitlint/config-conventional": "^20.0.0",
        "cross-env": "^10.1.0",
        "eslint": "^9.39.1",
        "husky": "^9.1.7",
        "rxjs": "^7.8.2",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">= 20"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
