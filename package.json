{
    "name": "@hug/eslint-config",
    "version": "16.0.0",
    "description": "ESLint shareable configuration with great defaults",
    "homepage": "https://github.com/dsi-hug/eslint-config",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dsi-hug/eslint-config.git"
    },
    "main": "./recommended.js",
    "exports": {
        ".": {
            "default": "./recommended.js",
            "require": "./recommended.js"
        },
        "./recommended": {
            "default": "./recommended.js",
            "require": "./recommended.js"
        },
        "./moderate": {
            "default": "./moderate.js",
            "require": "./moderate.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "README.md",
        "LICENSE",
        "_base.js",
        "recommended.js",
        "moderate.js",
        "rules"
    ],
    "keywords": [
        "eslint",
        "eslintconfig",
        "lint",
        "linter",
        "code",
        "style",
        "quality",
        "angular",
        "rxjs",
        "javascript",
        "jslint",
        "es6",
        "typescript",
        "cypress"
    ],
    "scripts": {
        "prepare": "husky install",
        "ncu": "npx npm-check-updates -i --format=group",
        "test": "eslint --config ./tests/moderaterc.json ./tests && eslint --config ./tests/recommendedrc.json ./tests"
    },
    "peerDependencies": {
        "@angular/core": ">= 10",
        "eslint": ">= 8",
        "rxjs": ">= 6",
        "typescript": ">= 3"
    },
    "dependencies": {
        "@angular-eslint/builder": "16.2.0",
        "@angular-eslint/eslint-plugin": "16.2.0",
        "@angular-eslint/eslint-plugin-template": "16.2.0",
        "@angular-eslint/template-parser": "16.2.0",
        "@rushstack/eslint-patch": "1.5.1",
        "@typescript-eslint/eslint-plugin": "6.8.0",
        "@typescript-eslint/parser": "6.8.0",
        "eslint-plugin-chai-friendly": "0.7.2",
        "eslint-plugin-cypress": "2.15.1",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-jsdoc": "46.8.2",
        "eslint-plugin-jsonc": "2.10.0",
        "eslint-plugin-no-loops": "0.3.0",
        "eslint-plugin-no-secrets": "0.8.9",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-rxjs-angular": "2.0.1",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-unused-imports": "3.0.0",
        "find-up": "5.0.0"
    },
    "devDependencies": {
        "@angular/core": "^16.2.9",
        "@commitlint/cli": "^17.8.0",
        "@commitlint/config-conventional": "^17.8.0",
        "eslint": "^8.51.0",
        "husky": "^8.0.3",
        "rxjs": "^7.8.1",
        "typescript": "^5.2.2"
    },
    "engines": {
        "node": "^16.14.0 || >=18.10.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
