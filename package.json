{
    "name": "@hug/eslint-config",
    "version": "13.0.0",
    "description": "ESLint shareable configuration with great defaults",
    "homepage": "https://github.com/dsi-hug/eslint-config",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dsi-hug/eslint-config.git"
    },
    "main": "./recommended.js",
    "exports": {
        ".": {
            "require": "./recommended.js"
        },
        "./recommended": {
            "require": "./recommended.js"
        },
        "./moderate": {
            "require": "./moderate.js"
        }
    },
    "files": [
        "README.md",
        "LICENSE",
        "_base.js",
        "recommended.js",
        "moderate.js",
        "rules"
    ],
    "keywords": [
        "eslint",
        "eslintconfig",
        "lint",
        "linter",
        "code",
        "style",
        "quality",
        "angular",
        "rxjs",
        "javascript",
        "jslint",
        "es6",
        "typescript",
        "cypress"
    ],
    "scripts": {
        "prepare": "husky install",
        "ncu": "npx npm-check-updates -i --format=group",
        "test": "eslint --config ./tests/moderaterc.json ./tests && eslint --config ./tests/recommendedrc.json ./tests"
    },
    "peerDependencies": {
        "@angular/core": ">= 10",
        "eslint": ">= 8",
        "rxjs": ">= 6",
        "typescript": ">= 3"
    },
    "dependencies": {
        "@angular-eslint/builder": "15.2.1",
        "@angular-eslint/eslint-plugin": "15.2.1",
        "@angular-eslint/eslint-plugin-template": "15.2.1",
        "@angular-eslint/template-parser": "15.2.1",
        "@rushstack/eslint-patch": "1.2.0",
        "@typescript-eslint/eslint-plugin": "5.58.0",
        "@typescript-eslint/parser": "5.58.0",
        "eslint-plugin-chai-friendly": "0.7.2",
        "eslint-plugin-cypress": "2.13.2",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsdoc": "41.1.1",
        "eslint-plugin-jsonc": "2.7.0",
        "eslint-plugin-no-loops": "0.3.0",
        "eslint-plugin-no-secrets": "0.8.9",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-rxjs-angular": "2.0.0",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-unused-imports": "2.0.0",
        "find-up": "5.0.0"
    },
    "devDependencies": {
        "@angular/core": "^15.2.6",
        "@commitlint/cli": "^17.5.1",
        "@commitlint/config-conventional": "^17.4.4",
        "eslint": "^8.38.0",
        "husky": "^8.0.3",
        "rxjs": "^7.8.0",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">= 14.14.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
