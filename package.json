{
    "name": "@hug/eslint-config",
    "version": "20.3.3",
    "description": "ESLint shareable configuration with great defaults",
    "homepage": "https://github.com/dsi-hug/eslint-config",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsi-hug/eslint-config.git"
    },
    "type": "module",
    "main": "./index.js",
    "typings": "./index.d.ts",
    "exports": {
        ".": {
            "require": "./index.js",
            "types": "./index.d.ts",
            "default": "./index.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "README.md",
        "LICENSE",
        "index.js",
        "index.d.ts",
        "configs",
        "scripts/deprecated.mjs"
    ],
    "keywords": [
        "eslint",
        "eslintconfig",
        "lint",
        "linter",
        "code",
        "style",
        "quality",
        "angular",
        "rxjs",
        "javascript",
        "jslint",
        "es6",
        "typescript",
        "cypress"
    ],
    "scripts": {
        "postinstall": "node scripts/deprecated.mjs",
        "ncu": "npx npm-check-updates -i --format=group",
        "inspect": "npx @eslint/config-inspector",
        "prepare": "husky || true",
        "lint": "eslint .",
        "test": "cross-env CONFIG=MODERATE eslint tests && cross-env CONFIG=RECOMMENDED eslint tests",
        "test:ci": "npm run test",
        "release": "npm publish --access public"
    },
    "peerDependencies": {
        "@angular/core": ">= 10",
        "eslint": ">= 9",
        "rxjs": ">= 6",
        "typescript": ">= 3"
    },
    "dependencies": {
        "angular-eslint": "18.4.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-chai-friendly": "1.0.1",
        "eslint-plugin-cypress": "4.0.0",
        "eslint-plugin-jsdoc": "50.4.3",
        "eslint-plugin-jsonc": "2.16.0",
        "eslint-plugin-no-loops": "0.4.0",
        "eslint-plugin-no-secrets": "1.0.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-rxjs-angular-updated": "^1.0.12",
        "eslint-plugin-rxjs-updated": "^1.0.12",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-unused-imports": "4.1.4",
        "globals": "^15.11.0",
        "typescript-eslint": "8.11.0"
    },
    "devDependencies": {
        "@angular/core": "^18.2.9",
        "@commitlint/cli": "^19.5.0",
        "@commitlint/config-conventional": "^19.5.0",
        "@types/eslint__js": "^8.42.3",
        "cross-env": "^7.0.3",
        "eslint": "^9.13.0",
        "husky": "^9.1.6",
        "prettier": "^3.3.3",
        "prettier-plugin-multiline-arrays": "^3.0.6",
        "rxjs": "^7.8.1",
        "typescript": "^5.6.3"
    },
    "engines": {
        "node": ">=18.13.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
