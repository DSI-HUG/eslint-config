{
    "name": "@hug/eslint-config",
    "version": "20.1.0",
    "description": "ESLint shareable configuration with great defaults",
    "homepage": "https://github.com/dsi-hug/eslint-config",
    "license": "GPL-3.0-only",
    "author": "HUG - Hôpitaux Universitaires Genève",
    "contributors": [
        "badisi (https://github.com/badisi)"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dsi-hug/eslint-config.git"
    },
    "main": "./recommended.js",
    "exports": {
        ".": {
            "require": "./recommended.js",
            "default": "./recommended.js"
        },
        "./recommended": {
            "require": "./recommended.js",
            "default": "./recommended.js"
        },
        "./moderate": {
            "require": "./moderate.js",
            "default": "./moderate.js"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "README.md",
        "LICENSE",
        "_base.js",
        "recommended.js",
        "moderate.js",
        "rules"
    ],
    "keywords": [
        "eslint",
        "eslintconfig",
        "lint",
        "linter",
        "code",
        "style",
        "quality",
        "angular",
        "rxjs",
        "javascript",
        "jslint",
        "es6",
        "typescript",
        "cypress"
    ],
    "scripts": {
        "ncu": "npx npm-check-updates -i --format=group",
        "prepare": "husky || true",
        "test": "cd tests && eslint --config moderaterc.json *.{ts,js,html,json} && eslint --config recommendedrc.json *.{ts,js,html,json}",
        "test:ci": "npm run test",
        "release": "npm publish --access public"
    },
    "peerDependencies": {
        "@angular/core": ">= 10",
        "eslint": ">= 8",
        "rxjs": ">= 6",
        "typescript": ">= 3"
    },
    "dependencies": {
        "@angular-eslint/builder": "18.2.0",
        "@angular-eslint/eslint-plugin": "18.2.0",
        "@angular-eslint/eslint-plugin-template": "18.2.0",
        "@angular-eslint/template-parser": "18.2.0",
        "@rushstack/eslint-patch": "1.10.4",
        "@stylistic/eslint-plugin": "^2.6.2",
        "@typescript-eslint/eslint-plugin": "8.0.1",
        "@typescript-eslint/parser": "8.0.1",
        "eslint-plugin-chai-friendly": "1.0.1",
        "eslint-plugin-cypress": "3.4.0",
        "eslint-plugin-import": "2.29.1",
        "eslint-plugin-jsdoc": "50.0.1",
        "eslint-plugin-jsonc": "2.16.0",
        "eslint-plugin-no-loops": "0.3.0",
        "eslint-plugin-no-secrets": "1.0.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-rxjs-angular": "2.0.1",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-unused-imports": "4.1.3",
        "find-up": "5.0.0"
    },
    "devDependencies": {
        "@angular/core": "^18.1.4",
        "@commitlint/cli": "^19.4.0",
        "@commitlint/config-conventional": "^19.2.2",
        "eslint": "^8.56.0",
        "husky": "^9.1.4",
        "rxjs": "^7.8.1",
        "typescript": "^5.5.4"
    },
    "engines": {
        "node": ">=18.13.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    }
}
